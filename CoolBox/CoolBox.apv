[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
Util.BatchError     PROCEDURE(STRING TraceMessage)
    CODE
    MESSAGE(BatchErrMsg, |
    BatchErrTitle, |
    ICON:Hand, |
    BUTTON:OK, |
    BUTTON:OK, |
    MSGMODE:CANCOPY)
    CapeSoftBatchFile.Trace(TraceMessage)
[END]
