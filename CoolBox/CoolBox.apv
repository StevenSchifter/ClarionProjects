EMBED %DataSection
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 1300
PROPERTY:END
Util                 Utilities
[SOURCE]
PROPERTY:BEGIN
PRIORITY 3000
PROPERTY:END
SafeReaderPath       CSTRING(256)
InstallerPath        CSTRING(256)
FilesOpened          BYTE(0)
[END]
EMBED %ProcedureRoutines
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
OpenFiles ROUTINE
    Access:CapeSoftAccessories.Open()
    Access:CapeSoftAccessories.UseFile()
    FilesOpened = True
    CLEAR(CapeSoftAccessories)
    SET(CSA:InstallerNameKey,CSA:InstallerNameKey)
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
CloseFiles ROUTINE
    IF FilesOpened THEN
        Access:CapeSoftAccessories.Close()
        FilesOpened = False
    END
[END]
EMBED %OperationRunningCode
[INSTANCES]
WHEN '1'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
CASE Access:CapeSoftAccessories.Next()
OF Level:Notify OROF Level:Fatal
    StopOperation()
ELSE
    IF CSA:InstallerName AND CSA:InstallerPassword AND CSA:IncludeInGeneratedBatchFile
        CurrentOpInfo = 'Installing ' & CSA:InstallerName
        RUN(GenSafeReaderRunString(SafeReaderPath,InstallerPath,CSA:InstallerName,CSA:InstallerPassword),1)
    END
END
[END]
[END]
EMBED %WindowManagerMethodCodeSection
[INSTANCES]
WHEN 'Init'
[INSTANCES]
WHEN '(),BYTE'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 8400
PROPERTY:END
  INIMgr.Fetch('CapeSoftAccessoriesOptions','SafeReaderPath',SafeReaderPath)
  INIMgr.Fetch('CapeSoftAccessoriesOptions','InstallerPath',InstallerPath)
[SOURCE]
PROPERTY:BEGIN
PRIORITY 7501
PROPERTY:END
DO OpenFiles
[END]
[END]
WHEN 'Kill'
[INSTANCES]
WHEN '(),BYTE'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 6001
PROPERTY:END
DO CloseFiles
[END]
[END]
[END]
