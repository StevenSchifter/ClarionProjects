NAME GenCapeSoftBatch
[COMMON]
FROM ABC Source
MODIFIED '2022/08/20' '23:07:54'
[FILES]
[OTHERS]
CapeSoftAccessories
[PROMPTS]
%GenerateOpenClose LONG  (1)
%GenerateSaveRestore LONG  (0)
[EMBED]
EMBED %DataSection
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 1300
PROPERTY:END
CapeSoftBatchFile  StringTheory
Util               CLASS(Utilities)
BatchError             PROCEDURE(STRING TraceMessage)
                   END
[END]
EMBED %ProcessedCode
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
    DO OpenFiles
    DO GenerateBatch
    DO CloseFiles
    
[END]
EMBED %ProcedureRoutines
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 2600
PROPERTY:END
GenerateBatch       ROUTINE
    DATA
i               LONG
CsaRecs         LONG
    CODE
    INIMgr.Fetch('CapeSoftAccessoriesOptions','SafeReaderPath',SafeReaderPath)
    INIMgr.Fetch('CapeSoftAccessoriesOptions','InstallerPath',InstallerPath)
    BatchPath = Util.TrailingBackslash(InstallerPath) & 'InstallCapeSoftAccessories.bat'
    BatchErrMsg = 'No batch file has been generated.|Please make sure there are' & |
                  ' complete records in the CapeSoft Accessories table and that|' & |
                  BatchPath & '|and/or any of its containing folders allow file creation and/or modification.'
    CapeSoftBatchFile.Start()
    CsaRecs = RECORDS(CapeSoftAccessories)
    CLEAR(CapeSoftAccessories)
    SET(CSA:InstallerNameKey,CSA:InstallerNameKey)
    LOOP i = 1 TO CsaRecs
        CASE Access:CapeSoftAccessories.Next()
        OF Level:Notify OROF Level:Fatal
            BREAK
        ELSE
            IF CSA:InstallerName AND CSA:InstallerPassword AND CSA:IncludeInGeneratedBatchFile
                CapeSoftBatchFile.Append(GenSafeReaderRunString(SafeReaderPath,InstallerPath,CSA:InstallerName,CSA:InstallerPassword) |
                & CRLF & '@pause' & CRLF)
            END
        END
    END
    IF CapeSoftBatchFile.Length()
        CapeSoftBatchFile.Prepend('@echo off' & CRLF)
        CapeSoftBatchFile.Append('exit' & CRLF)
        IF CapeSoftBatchFile.SaveFile(BatchPath)
            MESSAGE('A batch file has been generated at:|' & BatchPath, |
            'Batch File Generated', |
            ICON:Asterisk, |
            BUTTON:OK, |
            MSGMODE:CANCOPY)
        ELSE
            Util.BatchError('Save Failed')
        END
    ELSE
        Util.BatchError('Empty File')
    END
[END]
