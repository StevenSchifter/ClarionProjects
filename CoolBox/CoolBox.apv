!    CODE
    CLEAR(UtestQ)
    UTQ:CodeToTest = 'Util.MaxVal(6,3)'
    UTQ:ExpectedResult = '6'
    UTQ:ActualResult = Util.MaxVal(6,3)
    ADD(UtestQ)
    CLEAR(UtestQ)
    UTQ:CodeToTest = 'Util.MinVal(6,3)'
    UTQ:ExpectedResult = '3'
    UTQ:ActualResult = Util.MinVal(6,3)
    ADD(UtestQ)
    CLEAR(UtestQ)
    UTQ:CodeToTest = 'Util.TrailingBackslash(getCSIDLfolder.GetDir(SV:CSIDL_DESKTOP))'
    UTQ:ExpectedResult = 'C:\Users\Steven\Desktop\'
    UTQ:ActualResult = Util.TrailingBackslash(getCSIDLfolder.GetDir(SV:CSIDL_DESKTOP))
    ADD(UtestQ)
    
[END]
[END]
