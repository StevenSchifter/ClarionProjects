[EMBED]
EMBED %LocalProcedures
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
PSFR.DeleteExistingRecords    PROCEDURE()
RecordsDeleted    LONG
    CODE
    CLEAR(SoundFiles.Record,1)
    SoundFile:FolderID = SoundFolder:ID
    SET(SoundFile:KeyFolderFile,SoundFile:KeyFolderFile)
    LOOP
        PREVIOUS(SoundFiles)
        IF SoundFile:FolderID = SoundFolder:ID
            DELETE(SoundFiles)
            RecordsDeleted += 1
        ELSE
            BREAK
        END
    END
    IF RecordsDeleted > 0
        ?InfoString{PROP:Text} = RecordsDeleted & ' existing file records deleted.'
        DISPLAY()
        SLEEP(3000)
    END
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
PSFR.GetLastRecordNumber    PROCEDURE()!,LONG
ReturnValue    LONG
    CODE
    IF RECORDS(SoundFiles)
        CLEAR(SoundFiles.Record,1)
        SET(SoundFile:KeyID,SoundFile:KeyID)
        PREVIOUS(SoundFiles)
        ReturnValue = SoundFile:ID
    ELSE
        ReturnValue = 0
    END
    RETURN ReturnValue
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
PSFR.PopulateFileRecords    PROCEDURE()
OriginalPath    STRING(FILE:MaxFilePath)
FileQ           QUEUE(FILE:Queue),PRE(FQ)
                END
LastRecord      LONG
Qrecords        LONG
Qindex          LONG
RecordsAdded    LONG
CloseCountdown  LONG
    CODE
    Access:SoundFiles.Open()
    OriginalPath = LONGPATH()
    SELF.DeleteExistingRecords()
    LastRecord = SELF.GetLastRecordNumber()
    CLEAR(SoundFiles.Record)
    IF Extensions:WAV THEN DIRECTORY(FileQ,Util.TrailingBackslash(SoundFolder:Name) & '*.wav',ff_:NORMAL) END
    IF Extensions:AVI THEN DIRECTORY(FileQ,Util.TrailingBackslash(SoundFolder:Name) & '*.avi',ff_:NORMAL) END
    IF Extensions:MP3 THEN DIRECTORY(FileQ,Util.TrailingBackslash(SoundFolder:Name) & '*.mp3',ff_:NORMAL) END
    IF Extensions:WMA THEN DIRECTORY(FileQ,Util.TrailingBackslash(SoundFolder:Name) & '*.wma',ff_:NORMAL) END
    IF Extensions:MID THEN DIRECTORY(FileQ,Util.TrailingBackslash(SoundFolder:Name) & '*.mid',ff_:NORMAL) END
    IF Extensions:RMI THEN DIRECTORY(FileQ,Util.TrailingBackslash(SoundFolder:Name) & '*.rmi',ff_:NORMAL) END
    SETPATH(CLIP(OriginalPath))
    Qrecords = RECORDS(FileQ)
    IF Qrecords
        LOOP Qindex = 1 TO Qrecords
            GET(FileQ,Qindex)
            IF ERRORCODE()
                STOP(ERROR())
                BREAK
            END
            LastRecord += 1
            SoundFile:ID = LastRecord
            SoundFile:FolderID = SoundFolder:ID
            SoundFile:Name = FQ:Name
            ADD(SoundFiles)
            IF ERRORCODE()
                STOP(ERROR())
                BREAK
            END
            RecordsAdded += 1
        END
    END
    Access:SoundFiles.Close()
    LOOP CloseCountdown = 5 TO 1 BY -1 
        ?InfoString{PROP:Text} = RecordsAdded & ' file records populated. ' & |
                               'Closing in ' & CloseCountdown
        DISPLAY()
        SLEEP(1000)
    END
[END]
