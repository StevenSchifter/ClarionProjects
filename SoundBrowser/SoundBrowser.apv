          FONT:regular,CHARSET:DEFAULT),DOUBLE,CENTER,GRAY,IMM,HLP('~ManageSoundFolders' & |
          '.htm#PopulateSoundFileRecords'),SYSTEM
          GROUP('Extensions to Populate'),AT(5,5,160,85),USE(?ExtensionsGroup),BOXED,TRN,#ORDINAL(1)
            CHECK('WAV'),AT(55,16,36),USE(Extensions:WAV),MSG('Microsoft Wave Sound Format'), |
              TIP('Microsoft Wave Sound Format'),#ORDINAL(2)
            CHECK('AVI'),AT(55,29,36),USE(Extensions:AVI),MSG('Microsoft Audio Video In' & |
              'terleave Format'),TIP('Microsoft Audio Video Interleave Format'),#ORDINAL(3)
            CHECK('MP3'),AT(55,41,36),USE(Extensions:MP3),MSG('MPEG Layer 3 Audio File ' & |
              '(Constant bit rate only)'),TIP('MPEG Layer 3 Audio File (Constant bit rate only)'), |
              #ORDINAL(4)
            CHECK('WMA'),AT(55,53,36),USE(Extensions:WMA),MSG('Windows Media Audio Format'), |
              TIP('Windows Media Audio Format'),#ORDINAL(5)
            CHECK('MID'),AT(55,65,36),USE(Extensions:MID),MSG('MIDI File Format'),TIP('MIDI File Format'), |
              #ORDINAL(6)
            CHECK('RMI'),AT(55,76,36),USE(Extensions:RMI),MSG('RIFF-based MIDI File Format'), |
              TIP('RIFF-based MIDI File Format'),#ORDINAL(7)
          END
          BUTTON('&OK'),AT(2,105,52,14),USE(?Ok),FONT(,,,FONT:bold),LEFT,ICON('WAOK.ICO'), |
            FLAT,MSG('Accept operation'),TIP('Accept Operation'),#ORIG(?Ok),#SEQ(1),#ORDINAL(8)
          BUTTON('&Cancel'),AT(60,105,52,14),USE(?Cancel),FONT(,,,FONT:bold),LEFT,ICON('WACANCEL.ICO'), |
            FLAT,MSG('Cancel Operation'),TIP('Cancel Operation'),#ORIG(?Cancel),#SEQ(2),#ORDINAL(9)
          BUTTON('&Help'),AT(117,105,52,14),USE(?Help),FONT(,,,FONT:bold),LEFT,ICON('WIZHELP.ICO'), |
            FLAT,MSG('See Help Window'),STD(STD:Help),TIP('See Help Window'),#ORIG(?Help), |
            #SEQ(3),#ORDINAL(10)
          STRING('Click OK to populate sound file records.'),AT(18,92),USE(?InfoString),CENTER, |
            TRN,#ORDINAL(11)
        END

[END]
