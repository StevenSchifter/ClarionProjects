[EMBED]
EMBED %DataSection
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 50
PROPERTY:END
Util          Utilities
[SOURCE]
PROPERTY:BEGIN
PRIORITY 1300
PROPERTY:END
PlaybackControls    CLASS
PlayingMultiple         BYTE
TimeToStop              BYTE
PlayMultiple            PROCEDURE(BYTE MarkedOnly)
PlayOne                 PROCEDURE()
StopPlaying             PROCEDURE()
ToggleMark              PROCEDURE()
                    END
[END]
EMBED %LocalProcedures
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
PlaybackControls.PlayMultiple PROCEDURE(BYTE MarkedOnly)
    CODE
    SELF.PlayingMultiple = TRUE
    CLEAR(SoundFiles)
    SoundFile:FolderID = Queue:Browse:1.SoundFile:FolderID
    SET(SoundFiles,SoundFile:KeyFolderFile)
    LOOP UNTIL EOF(SoundFiles)
        IF SELF.TimeToStop
            SELF.PlayingMultiple = FALSE
            SELF.TimeToStop      = FALSE
            BREAK
        END
        NEXT(SoundFiles)
        IF MarkedOnly AND NOT SoundFile:FileMarked
            CYCLE
        END
        SELF.PlayOne()
        !Wait until playback has stopped before iterating the loop.
        SLEEP(CurrentSoundLength)
    END
    SELF.PlayingMultiple = FALSE
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
PlaybackControls.PlayOne    PROCEDURE()
    CODE
    StopSound(CurrentSound)
    CloseSound(CurrentSound)
    CurrentSound = Util.TrailingBackslash(SoundFolder:Name) & SoundFile:Name
    IF GoodToGo = OpenSound(CurrentSound)
        CurrentSoundLength = GetSoundLength(CurrentSound)
        DISPLAY()
        PlaySound(CurrentSound)
    END
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
PlaybackControls.StopPlaying PROCEDURE()
    CODE
    IF SELF.PlayingMultiple
       SELF.TimeToStop = TRUE
    END
    StopSound(CurrentSound)
    CloseSound(CurrentSound)
    CurrentSound = ''
    CurrentSoundLength = ''
    DISPLAY()
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
PlaybackControls.ToggleMark PROCEDURE()
    CODE
    SoundFile:FolderID = Queue:Browse:1.SoundFile:FolderID
    SoundFile:Name = Queue:Browse:1.SoundFile:Name
    GET(SoundFiles,SoundFile:KeyFolderFile)
    IF NOT SoundFile:FileMarked
        SoundFile:FileMarked = '>'
    ELSE
        SoundFile:FileMarked = ''
    END
    PUT(SoundFiles)
    ThisWindow.Reset(TRUE)
[END]
