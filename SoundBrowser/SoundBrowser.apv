[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
    IF MySoundFile
        QuotedFileName = '"' & CLIP(MySoundFile) & '"'
        CommandString = 'open ' & QuotedFileName
        ReturnValue = mciSendString(CommandString,ReturnString,SIZE(ReturnString),GetActiveWindow())
        IF ReturnValue <> 0
            MESSAGE('Could not open ' & QuotedFileName & ':|' & GetSoundError(ReturnValue),'Sound File Open Error',ICON:Hand)
        END
    END
    RETURN ReturnValue
