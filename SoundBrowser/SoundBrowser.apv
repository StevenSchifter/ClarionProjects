EMBED %LocalProcedures
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
USF.BrowseForFile    PROCEDURE()
DialogTitle    STRING('Select a sound file')
Extensions     STRING('WAV files|*.wav|' & | 
                      'AVI files|*.avi|' & | 
                      'MIDI files|*.mid|' & | 
                      'MP3 files|*.mp3|' & | 
                      'WMA files|*.wma|' & | 
                      'All files|*.*')
OriginalPath   STRING(FILE:MaxFilePath)
    CODE
    OriginalPath = LONGPATH()
    SETPATH(SoundFolder:Name)
    IF FILEDIALOG(DialogTitle,SoundFile:Name,Extensions,FILE:LongName)
        SoundFile:Name = Util.GetFileName(SoundFile:Name)
    END
    SETPATH(CLIP(OriginalPath))
    ThisWindow.Reset(TRUE)
[END]
[END]
