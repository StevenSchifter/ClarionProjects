EMBED %LocalProcedures
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 3600
PROPERTY:END
USF.BrowseForFolder    PROCEDURE()
DialogTitle    STRING('Select a folder with sound files')
    CODE
    FILEDIALOG(DialogTitle,SoundFolder:Name,,FILE:LongName + FILE:Directory)
    ThisWindow.Reset(TRUE)
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
USF.CheckFolderExistence    PROCEDURE()
    CODE
    IF EXISTS(SoundFolder:Name)
        ?ErrorString{PROP:Text} = ''
        RETURN TRUE
    ELSE
        ?ErrorString{PROP:Text} = 'ERROR: Folder not found'
        RETURN FALSE
    END
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
USF.OpenFolder    PROCEDURE()
    CODE
    IF SELF.CheckFolderExistence()
        RUN('explorer "' & SoundFolder:Name & '"')
        IF RUNCODE()
            ?ErrorString{PROP:Text} = 'Folder Open Error: ' & ERROR()
        ELSE
            ?ErrorString{PROP:Text} = ''
        END
    END
[END]
EMBED %DataSection
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4500
PROPERTY:END
USF                 CLASS
FolderExists            BYTE
BrowseForFolder         PROCEDURE()
CheckFolderExistence    PROCEDURE(),BYTE
OpenFolder              PROCEDURE()
                    END
[END]
